# データベース設定
# ローカル開発時
# DATABASE_URL=postgresql://radio_user:radio_password@localhost:5432/radio_corner_selector
# Docker環境時（docker-compose.yamlで自動設定）
DATABASE_URL=postgresql://radio_user:radio_password@db:5432/radio_corner_selector

# Google Gemini API
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_MODEL=gemini-pro

# アプリケーション設定
APP_NAME="Radio Corner Selector API"
DEBUG=True

# CORS設定（カンマ区切り）
CORS_ORIGINS=http://localhost:8501,http://localhost:3000
